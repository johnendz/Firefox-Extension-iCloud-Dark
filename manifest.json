{
  "description": "Ativar modo escuro no iCloud",
  "manifest_version": 2,
  "name": "iCloud Dark",
  "author": "Johnendz",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://www.icloud.com/*"],
      "css":["style.css"],
      "js": ["jquery.min.js", "script.js"],
      "run_at": "document_start"
    }
  ],
  
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  }
}